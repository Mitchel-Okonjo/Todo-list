{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": 0,
    "import/prefer-default-export": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "no-plusplus": "off"
  }
}
